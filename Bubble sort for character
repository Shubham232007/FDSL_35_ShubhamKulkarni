#include <stdio.h> 
int main() 
{ 
    int n, i, j; 
    char temp; 
    printf("Enter size of array: "); scanf("%d", &n); 
    char a[n];

printf("Enter array elements:\n");
for (i = 0; i < n; i++) {
    printf("Enter element %d: ", i + 1);
    scanf(" %c",&a[i]);
}

for (i = 0; i < n - 1; i++) {
    for (j = 0; j < n - i - 1; j++) { 
        if (a[j] > a[j + 1]) {
            temp = a[j];
            a[j] = a[j + 1];
            a[j + 1] = temp;
        }
    }
}
printf("Sorted elements are:\n");
for (i = 0; i < n; i++) {
    printf("%c\t", a[i]);
}
return 0;

}
